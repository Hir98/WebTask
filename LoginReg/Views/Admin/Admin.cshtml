@model IEnumerable<LoginReg.Models.User>
    @{
        ViewBag.Title = "Admin";
    }

    <h2>Admin</h2>

    <!DOCTYPE html>

    <html>
    <head>
        <meta name="viewport" content="width=device-width" />
        <title>Index</title>
    </head>
    <body>
        <h4>Admin</h4>
        <hr />
        <table cellpadding="0" cellspacing="0">
            <tr>
                <th>UserId</th>
                <th>UserName</th>
                <th>City</th>
                <th>Country</th>
            </tr>
            @foreach (Users user in Model)
            {
                <tr>
                    <td>@user.UserId</td>
                    <td>@user.UserName</td>
                    <td>@user.City</td>
                    <td>@user.Country</td>
                </tr>
            }
        </table>
    </body>
</html>
